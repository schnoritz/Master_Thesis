	journaltitle = {Current Directions in Biomedical Engineering},
	shortjournal = {Current Directions in Biomedical Engineering},
	author = {Richter, Sebastian and Pojtinger, Stefan and Mönnich, David and Dohm, Oliver and Thorwarth, Daniela},
	date = {2017-09-07},
