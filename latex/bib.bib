	keywords = {87.53.10.b, Angular distribution, Collimation, Collision theories, Dosimetry/exposure assessment, {GAMMA} {DOSIMETRY}, {MONTE} {CARLO} {METHOD}, Monte Carlo methods, Photon scattering, Photons, {RADIATION} {DOSE} {DISTRIBUTIONS}, {RADIATION} {DOSES}, {SCATTERING}, {TISSUE}−{EQUIVALENT} {MATERIALS}, Treatment strategy, {WATER}, X−{RAY} {DOSIMETRY}},
