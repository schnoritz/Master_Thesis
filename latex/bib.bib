	author = {Lundstedt, Dan and Gustafsson, Magnus and Steineck, Gunnar and Alsadius, David and Sundberg, Agnetha and Wilderäng, Ulrica and Holmberg, Erik and Johansson, Karl-Axel and Karlsson, Per},
